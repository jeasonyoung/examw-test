<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- service -->
	<!-- 考试大纲服务接口实现 begin-->
	<!-- 出版社服务 -->
	<bean id="pressService" class="com.examw.test.service.syllabus.impl.PressServiceImpl">
		<!-- 出版社数据接口 -->
		<property name="pressDao" ref="pressDao"/>
	</bean>
	<!-- 大纲服务 -->
	<bean id="syllabusService" class="com.examw.test.service.syllabus.impl.SyllabusServiceImpl">
		<!-- 大纲数据接口 -->
		<property name="syllabusDao" ref="syllabusDao"/>
		<!-- 科目数据接口 -->
		<property name="subjectDao" ref="subjectDao"/>
	</bean>
	<!-- 教材服务 -->
	<bean id="textbookService" class="com.examw.test.service.syllabus.impl.TextBookServiceImpl">
		<!-- 教材数据接口 -->
		<property name="textBookDao" ref="textBookDao"/>
		<!-- 科目数据接口 -->
		<property name="subjectDao" ref="subjectDao"/>
		<!-- 出版社数据接口 -->
		<property name="pressDao" ref="pressDao"/>
		<!-- 知识点数据接口 -->
		<property name="knowledgeService" ref="knowledgeService"/>
	</bean>
	<!-- 知识点服务 -->
	<bean id="knowledgeService" class="com.examw.test.service.syllabus.impl.KnowledegeServiceImpl">
		<!-- 知识点数据接口 -->
		<property name="knowledgeDao" ref="knowledgeDao"/>
		<!-- 考试大纲数据接口 -->
		<property name="syllabusDao" ref="syllabusDao" />
		<!-- 教材数据接口 -->
		<property name="textBookDao" ref="textBookDao" />
	</bean>
	<!-- 考试大纲 服务接口实现 end-->
</beans>