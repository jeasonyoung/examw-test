<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.examw.test.junit.domain">
	<!-- 来源管理 -->
	<class name="Question" table="ES_ShiTi">
		<!-- 来源ID -->
		<id name="id" column="ID"  type="string"  length="64" />
		<!-- 来源代码 -->
		<property name="type" type="integer" column="TypeID" />
		<!-- 来源名称 -->
		<property name="content" type="string" column="Content" />
		
		<property name="analysis" type="string" column="Analysis" />
		
		<property name="answer" type="string"  column="Answer"/>
	</class>
</hibernate-mapping>